{
  "name": "fanfou-weapp",
  "version": "1.1.5",
  "description": "Fanfou WeApp",
  "main": "app.js",
  "scripts": {
    "test": "xo",
    "upload": "cli/upload.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanfoujs/fanfou-weapp.git"
  },
  "author": "FanfouJS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fanfoujs/fanfou-weapp/issues"
  },
  "homepage": "https://github.com/fanfoujs/fanfou-weapp#readme",
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "globals": [
      "App",
      "wx",
      "Page",
      "getApp",
      "getCurrentPages"
    ],
    "ignores": [
      "src/utils/fanfou-sdk-node/modules/**"
    ],
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "import/no-unresolved": 0,
      "max-params": [
        "error",
        6
      ],
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ]
    },
    "semicolon": false,
    "space": 2
  },
  "dependencies": {
    "execa": "^0.8.0",
    "ora": "^1.3.0"
  }
}
