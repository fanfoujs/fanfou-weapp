{
  "name": "xiaofan",
  "version": "1.4.1",
  "description": "Fanfou WeApp",
  "main": "app.js",
  "scripts": {
    "test": "xo && ava",
    "upload": "cli/upload.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanfoujs/xiaofan.git"
  },
  "author": "FanfouJS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fanfoujs/xiaofan/issues"
  },
  "homepage": "https://github.com/fanfoujs/xiaofan#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "deep-keys": "^0.5.0",
    "xo": "^0.20.0"
  },
  "xo": {
    "globals": [
      "App",
      "wx",
      "Page",
      "getApp",
      "getCurrentPages"
    ],
    "ignores": [
      "src/utils/fanfou-sdk-node/modules/**"
    ],
    "rules": {
      "camelcase": 0,
      "import/no-unresolved": 0,
      "max-params": [
        "error",
        6
      ],
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "unicorn/import-index": 0
    },
    "semicolon": false,
    "space": 2
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "execa": "^0.9.0",
    "ora": "^1.3.0"
  }
}
